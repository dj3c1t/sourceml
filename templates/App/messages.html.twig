{% for label, flashes in app.session.flashbag.all %}
{%     for message in flashes %}
<div class="row">
    <div class="alert alert-{{ label }}">
        <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
        {{ message|trans }}
    </div>
</div>
{%     endfor %}
{% endfor %}
